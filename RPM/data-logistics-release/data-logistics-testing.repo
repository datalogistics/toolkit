[data-logistics-testing]
name=Data Logistics Testing Packages for CentOS - $basearch
baseurl=http://data-logistics.org/yum/testing/centos/$releasever/$basearch
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-data-logistics

